# Sequencer API endpoint (e.g., http://localhost:8080)
SEQUENCER_API_URL=

# Census API endpoint (e.g., http://localhost:8081)
CENSUS_API_URL=

# Sepolia RPC endpoint (e.g., from Infura, Alchemy, etc.)
SEPOLIA_RPC=

# Private key for transaction signing (without 0x prefix)
PRIVATE_KEY=

# (optional) The address of the organization registry contract
ORGANIZATION_REGISTRY_ADDRESS=

# (optional) The address of the process registry contract
PROCESS_REGISTRY_ADDRESS=

# Force using contract addresses from sequencer info endpoint (default: false)
FORCE_SEQUENCER_ADDRESSES=false

# (optional) CSP server URL for CSP census operations
# Required when using CSP census type - should point to a server that provides:
# GET /root - returns the current census root (JSON)
# GET /proof?pid=<hex>&addr=<0xaddr> - returns census inclusion proof
CSP_SERVER_URL=
